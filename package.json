{
  "name": "@medianaura/di-manager",
  "version": "1.0.4",
  "description": "",
  "repository": "https://github.com/MedianAura/di-manager",
  "license": "MIT",
  "author": "MedianAura",
  "type": "module",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "run-s -ln build:clean build:barrel build:tsc",
    "build:clean": "shx rm -rf dist",
    "build:barrel": "barrelsby -Hq --delete --location top --directory ./src",
    "build:tsc": "rollup -c --environment BUILD:production",
    "check": "concatenate check",
    "check:knip": "knip",
    "check:stryker": "stryker run",
    "fix": "concatenate fix",
    "release": "automaton version",
    "start": "node bin/run.js",
    "test:coverage": "open-cli coverage/lcov-report/index.html",
    "test:unit": "vitest --coverage.enabled --coverage.all --coverage.reporter=lcov",
    "test:unit:coverage": "vitest --reporter basic --reporter junit --outputFile.junit=junit.xml --coverage.enabled --coverage.reporter=text-summary --coverage.reporter=cobertura --coverage.reporter=lcov --watch=false",
    "watch": "rollup -c -w --environment BUILD:development"
  },
  "devDependencies": {
    "@medianaura/automaton": "^0.1.2",
    "@medianaura/concatenate": "^0.2.1",
    "@rollup/plugin-commonjs": "^26.0.1",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rushstack/eslint-patch": "^1.10.3",
    "@stryker-mutator/vitest-runner": "^8.2.6",
    "@swc/core": "^1.5.27",
    "@swc/helpers": "^0.5.11",
    "@types/node": "^20.14.2",
    "@typescript-eslint/eslint-plugin": "^7.12.0",
    "@typescript-eslint/parser": "^7.12.0",
    "barrelsby": "^2.8.1",
    "delay-cli": "^2.0.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-formatter-pretty": "^5.0.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-i": "^2.29.1",
    "eslint-plugin-n": "^17.8.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-promise": "^6.2.0",
    "eslint-plugin-regexp": "^2.6.0",
    "eslint-plugin-simple-import-sort": "^12.1.0",
    "eslint-plugin-unicorn": "52.0.0",
    "eslint-plugin-unused-imports": "^3.2.0",
    "eslint-plugin-zod": "^1.4.0",
    "knip": "^5.18.2",
    "npm-run-all2": "^6.2.0",
    "prettier": "^3.3.1",
    "rollup": "^4.18.0",
    "rollup-plugin-clear": "^2.0.7",
    "rollup-plugin-shell": "^1.0.9",
    "rollup-plugin-ts": "^3.4.5",
    "shx": "^0.3.4",
    "tsc-alias": "^1.8.10",
    "tsyringe": "^4.8.0",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.12.0",
    "unplugin-auto-import": "^0.17.6",
    "vitest": "^1.6.0"
  },
  "peerDependencies": {
    "tsyringe": ">=4.x"
  }
}
